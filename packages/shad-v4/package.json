{
  "name": "@rcls/elements-shadcn-twv4",
  "version": "0.0.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/lib/main.d.ts",
      "default": "./dist/main.js"
    },
    "./elements.css": {
      "require": "./dist/assets/elements-shadcn-twv4.css",
      "default": "./dist/assets/elements-shadcn-twv4.css"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:vite\"",
    "dev:vite": "vite",
    "dev:css": "npx @tailwindcss/cli -i ./index.css -o ./output.css --optimize --watch",
    "build": "npm run build:css && npm run build:vite",
    "build:vite": "vite build",
    "build:css": "npx @tailwindcss/cli -i ./index.css -o ./output.css --optimize --minify",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tailwindcss/cli": "^4.0.6",
    "@tailwindcss/vite": "^4.0.6",
    "@types/node": "^22.13.2",
    "@vitejs/plugin-react": "^4.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "glob": "^11.0.1",
    "globals": "^15.14.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
